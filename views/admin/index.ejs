<%- include('../partials/header') %>
<h1><a href="/items">> PRODUCTS <</a></h1>
<table id="list">
  <thead>
    <table>
      <tr>
          <H1>CURRENT USERS</H1>
      </tr>
      <tr>
          <h2>~~~~~~~~~~~~~~~~~~~~~~~~~~~~~</h2>
      </tr>
      <tr>
        <th></th>
        <th>Name</th>
        <th>Email</th>
      </tr>
  </thead>
  <tbody>
    <% users.forEach(function(u) { %>
      <tr>
        <td><img src="<%= u.avatar %>" width="25"></td>
        <td><%= u.name %></td>
        <td><%= u.email %></td>
        <td><a href="/user">DETAILS</a></td>
        <td><form name="userId" value="<%= u.id %>"
          action="/users?_method=DELETE" method="POST">
        <button type="submit">X</button>
        </form></td>
      </tr>
    <% }); %>
  </tbody>
</table>
<%- include('../partials/footer') %>