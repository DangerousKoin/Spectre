<%- include('../partials/header') %>
<table id="list">
  <thead>
    <tr>
      <th></th>
      <th>Name</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(function(u) { %>
      <tr>
        <td><img src="<%= u.avatar %>" width="25"></td>
        <td><%= u.name %></td>
        <td><%= u.email %></td>
        <td><a href="/users/<%= u._id %>">DETAILS</a></td>
        <td><form name="delUser" value="<%= u.id %>" action="/users/<%= u._id %>?_method=DELETE" method="POST">
          
        <button type="submit">X</button>
        </form></td>
      </tr>
    <% }); %>
  </tbody>
</table>
<%- include('../partials/footer') %>